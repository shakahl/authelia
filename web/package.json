    "husky": "8.0.3",
    "prettier": "2.8.4",
    "react-test-renderer": "18.2.0",
    "typescript": "4.9.5",
    "vite": "4.1.2",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-istanbul": "4.0.0",
